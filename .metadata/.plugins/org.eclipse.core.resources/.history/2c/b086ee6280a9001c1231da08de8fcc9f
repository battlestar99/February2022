package packageTwo;

public class LearningFunctions {

	public static void main(String[] args) {
		LearningFunctions learning = new LearningFunctions();
		learning.addTwoNumbers(10, 30);
		learning.addTwoNumbers(90, 60);
		learning.addTwoIntNumbers();
		
	}
	
	public void addTwoNumbers(int numberOne, int numberTwo) {
		int result = numberOne + numberTwo;
		System.out.println(result);
	}
	
	public void addTwoIntNumbers() {
		int a = 10;
		int b = 20;
		int c = a + b;
		System.out.println(c);
	}
	

}
