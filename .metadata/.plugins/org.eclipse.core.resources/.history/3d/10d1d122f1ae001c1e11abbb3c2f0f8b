package function;

public class FunctionLearning {

	public static void main(String[] args) {

		FunctionLearning fl = new FunctionLearning();
		
		Addition add = new Addition();
		add.addThreeNumbers(20, 20, 30);
		Multiplication multiply = new Multiplication();
		multiply.multiplyTwoNumbers(20, 30);
	}
	
	public void addTwoNumbers(int a, int b) {
		int c = a + b;
		System.out.println(c);
	}
	
	public void addThreeNumbers(int a, int b, int c) {
		int d = a + b+ c;
		System.out.println(d);
	}
	
	
	public void multiplyTwoNumbers(int a, int b) {
		int c = a * b;
		System.out.println(c);
	}
	
	public void multiplyThreeNumbers(int a, int b, int c) {
		int d = a * b * c;
		System.out.println(d);
	}
	

}
