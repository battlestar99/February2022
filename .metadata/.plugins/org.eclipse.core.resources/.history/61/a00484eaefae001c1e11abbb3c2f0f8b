package function;

public class FunctionLearning {

	public static void main(String[] args) {

		FunctionLearning fl = new FunctionLearning();
		fl.addTwoNumbers(10, 20);
		fl.addTwoNumbers(30, 40);
		fl.addTwoNumbers(20, 60);
		
		fl.addThreeNumbers(20, 40, 60);
	}
	
	public void addTwoNumbers(int a, int b) {
		int c = a + b;
		System.out.println(c);
	}
	
	public void addThreeNumbers(int a, int b, int c) {
		int d = a + b+ c;
		System.out.println(d);
	}
	

}
