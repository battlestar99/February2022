package packageOne;

public class ConditionalStatement {

	public static void main(String[] args) {
		
		String light = "green";
		
		if(light == "red") {
			System.out.println("stop!!");
		}else if(light == "green") {
			System.out.println("Go!!");
		}else if(light == "yellow") {
			System.out.println("slow down!!");
		}else {
			System.out.println("stop and go");
		}
		
	
	}

}
