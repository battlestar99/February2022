package packageTwo;

public class SampleClassTwo {

	public static void main(String[] args) {
		System.out.println("John");
		
		
		String name = "Zakir";
		
//		Primitve data type
		int phone = 571123489;
		int secondPhone = 0;
		double dollarAmount = 5.60;
		boolean isRaining = false;

		System.out.println(phone);
		
		phone = 57142646;
		System.out.println(phone);
		System.out.println(dollarAmount);
		System.out.println(isRaining);
//		float, long, char
		System.out.println(secondPhone);
		secondPhone = 15485616;
		System.out.println(secondPhone);
		phone = 514785;
		System.out.println(phone);
	}

	

}
