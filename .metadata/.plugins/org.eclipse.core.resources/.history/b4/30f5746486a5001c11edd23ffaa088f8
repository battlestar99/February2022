package packageTwo;

public class Operator {

	public static void main(String[] args) {
		
		//10 and 15
		int numberOne = 10;
		int numberTwo = 15;
		int sum = numberOne + numberTwo;
		System.out.println(sum);
		
		
		int numberThree = 25;
		int numberFour = 20;
		int sub = numberThree - numberFour;
		System.out.println(sub);
		
		int mult = numberThree * numberFour;
		System.out.println(mult);
		//Let's start again at 7:55
		
		
	}

}
