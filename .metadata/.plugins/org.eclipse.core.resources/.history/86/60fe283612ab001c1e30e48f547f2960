package packageTwo;

public class LearningFunctions {

	public static void main(String[] aslkfn) {
		LearningFunctions learning = new LearningFunctions();	
		learning.addTwoIntNumbers(25, 30);
		learning.addTwoIntNumbers(26, 20);
	}
	
	
	
	public void addTwoIntNumbers(int a, int b) {
		int c = a + b;
		System.out.println(c);
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	public void addTwoNumbers(int numberOne, int numberTwo) {
		int result = numberOne + numberTwo;
		System.out.println(result);
	}

}
